{
  "name": "trading-custody",
  "version": "1.0.0",
  "description": "A React and Express based trading and custody management system with user authentication, fund management, and position tracking",
  "keywords": [
    "react",
    "express",
    "trading",
    "custody",
    "finance",
    "portfolio",
    "management",
    "jwt",
    "sqlite"
  ],
  "homepage": ".",
  "bugs": {
    "url": "https://github.com/yushukk/trading-custody/issues"
  },
  "license": "MIT",
  "author": "Trading Custody System Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/yushukk/trading-custody.git"
  },
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "react-scripts start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "react-scripts build",
    "serve": "react-scripts build && npm install -g serve && serve -s build -p 3000",
    "test": "jest --config=jest.config.js",
    "test:watch": "jest --config=jest.config.js --watch",
    "test:coverage": "jest --config=jest.config.js --coverage",
    "test:server": "cd server && jest --config=tests/jest.config.js",
    "test:server:watch": "cd server && jest --config=tests/jest.config.js --watch",
    "test:server:coverage": "cd server && jest --config=tests/jest.config.js --coverage",
    "test:e2e": "cypress run --config-file tests/cypress/cypress.config.js",
    "test:e2e:open": "cypress open --config-file tests/cypress/cypress.config.js",
    "test:e2e:parallel": "cypress run --config-file tests/cypress/cypress.config.js --parallel --record --key YOUR_RECORD_KEY",
    "test:e2e:fast": "cypress run --config-file tests/cypress/cypress.config.js --headed --browser chrome",
    "test:e2e:headless": "cypress run --config-file tests/cypress/cypress.config.js --headless --browser electron",
    "test:e2e:spec": "cypress run --config-file tests/cypress/cypress.config.js --spec",
    "coverage": "npm run test:coverage && npm run test:server:coverage",
    "dev:local": "APP_PORT=${APP_PORT:-8085} SERVER_PORT=${SERVER_PORT:-3001} concurrently \"npm run server\" \"npm run client\"",
    "lint": "eslint src server --ext .js,.jsx",
    "lint:fix": "eslint src server --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css}\" \"server/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css}\" \"server/**/*.js\"",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "antd-mobile": "^5.39.0",
    "antd-mobile-icons": "^0.3.0",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "node-schedule": "^2.1.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.0",
    "sqlite3": "^5.1.7",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "@testing-library/cypress": "^10.1.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "concurrently": "^7.6.0",
    "cypress": "^12.17.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "http-proxy-middleware": "^3.0.5",
    "husky": "^9.1.7",
    "jest": "^27.5.1",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.0.2",
    "nyc": "^17.1.0",
    "prettier": "^3.7.4",
    "react-scripts": "^5.0.1",
    "supertest": "^7.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "proxy": "http://localhost:3001"
}
